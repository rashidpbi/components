
function sort(arr){

    function getValue(vehicleData){
        return vehicleData.speed;
    }

    return arr.sort((a,b)=>{

        
        let valueA = getValue(a);
        let valueB = getValue(b);

        sortOrder = 'asc';
        const sortValue = sortOrder==='asc'? 1 : -1;
        if (typeof valueA === "number"){

            return (valueA - valueB)*sortValue;
        }

        return (valueA.localeCompare(valueB)*sortValue);
    })
}